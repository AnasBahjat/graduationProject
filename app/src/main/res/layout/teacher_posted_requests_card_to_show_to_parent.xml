<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/custom_dialog_rounded_corners">

    <ImageView
        android:id="@+id/closeImageView"
        android:layout_width="28dp"
        android:layout_height="28dp"
        android:src="@drawable/close_icon_2"
        android:layout_marginStart="10dp"
        android:layout_marginTop="13dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"/>

    <ImageView
        android:id="@+id/profileImageView"
        android:layout_width="90dp"
        android:layout_height="125dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="50dp"
        android:src="@drawable/user" />

    <TextView
        android:id="@+id/childNameTextViewTemp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@id/profileImageView"
        android:layout_marginStart="10dp"
        android:layout_marginTop="60dp"
        android:text="Teacher Name : "
        android:textSize="16sp" />

    <TextView
        android:id="@+id/teacherNameTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginStart="7dp"
        android:layout_marginTop="60dp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="10dp"
        android:layout_toEndOf="@id/childNameTextViewTemp"
        android:textColor="#1A8E9E"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/parentNameTextViewTemp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/teacherNameTextView"
        android:layout_marginStart="10dp"
        android:layout_toEndOf="@id/profileImageView"
        android:layout_marginTop="10dp"
        android:text="Teacher Email : "
        android:textSize="16sp" />

    <TextView
        android:id="@+id/teacherEmailTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/teacherNameTextView"
        android:layout_marginStart="7dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/parentNameTextViewTemp"
        android:textColor="#1A8E9E"
        android:textSize="16sp" />


    <TextView
        android:id="@+id/parentPhoneNumberTextViewTemp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/teacherEmailTextView"
        android:layout_marginStart="10dp"
        android:layout_toEndOf="@id/profileImageView"
        android:layout_marginTop="10dp"
        android:text="Teacher Phone : "
        android:textSize="16sp" />

    <TextView
        android:id="@+id/teacherPhoneNumberTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/teacherEmailTextView"
        android:layout_marginStart="7dp"
        android:layout_marginTop="10dp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="10dp"
        android:layout_toEndOf="@+id/parentNameTextViewTemp"
        android:textColor="#1A8E9E"
        android:textSize="16sp" />


    <LinearLayout
        android:id="@+id/tempView1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:layout_below="@id/teacherPhoneNumberTextView"
        android:orientation="horizontal">

        <View
            android:layout_width="20dp"
            android:layout_height="2dp"
            android:layout_gravity="center"
            android:layout_marginStart="6dp"
            android:layout_marginTop="2dp"
            android:layout_marginEnd="1dp"
            android:background="#7FE1EE" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="5dp"
            android:text="Additional Information"
            android:textSize="22sp" />


        <View
            android:layout_width="190dp"
            android:layout_height="2dp"
            android:layout_gravity="center"
            android:layout_marginStart="7dp"
            android:layout_marginTop="2dp"
            android:layout_marginRight="15dp"
            android:background="#7FE1EE" />
    </LinearLayout>

    <ImageView
        android:id="@+id/coursesImageView"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:src="@drawable/courses_icon"
        android:layout_below="@id/tempView1"
        android:layout_marginTop="20dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="15dp"/>

    <TextView
        android:id="@+id/coursesTextViewTemp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Courses : "
        android:textSize="16sp"
        android:layout_below="@id/tempView1"
        android:layout_toEndOf="@id/coursesImageView"
        android:layout_marginTop="20dp"
        android:layout_marginStart="10dp"/>

    <TextView
        android:id="@+id/coursesTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:layout_below="@id/tempView1"
        android:layout_toEndOf="@id/coursesTextViewTemp"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="20dp"
        android:layout_marginStart="7dp"
        android:layout_marginEnd="10dp"
        android:textColor="#1A8E9E"/>


    <ImageView
        android:id="@+id/teachingMethodImageView"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:src="@drawable/teaching_method_icon_2"
        android:layout_below="@id/coursesTextView"
        android:layout_alignParentStart="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        />
    <TextView
        android:id="@+id/teachingMethodTextViewTemp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Teaching Method : "
        android:textSize="16sp"
        android:layout_below="@id/coursesTextView"
        android:layout_toEndOf="@id/teachingMethodImageView"
        android:layout_marginTop="20dp"
        android:layout_marginStart="10dp"/>

    <TextView
        android:id="@+id/teachingMethodTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:layout_below="@id/coursesTextView"
        android:layout_toEndOf="@id/teachingMethodTextViewTemp"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="20dp"
        android:layout_marginStart="7dp"
        android:layout_marginEnd="10dp"
        android:textColor="#1A8E9E"/>


    <ImageView
        android:id="@+id/timeImageView"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:src="@drawable/duration_image"
        android:layout_below="@id/teachingMethodTextView"
        android:layout_alignParentStart="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        />
    <TextView
        android:id="@+id/durationTextViewTemp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Duration : "
        android:textSize="16sp"
        android:layout_below="@id/teachingMethodTextView"
        android:layout_toEndOf="@id/timeImageView"
        android:layout_marginTop="20dp"
        android:layout_marginStart="10dp"/>

    <TextView
        android:id="@+id/durationTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:layout_below="@id/teachingMethodTextView"
        android:layout_toEndOf="@id/durationTextViewTemp"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="20dp"
        android:layout_marginStart="7dp"
        android:layout_marginEnd="10dp"
        android:textColor="#1A8E9E"
        />


    <ImageView
        android:id="@+id/startEndTimeImageView"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:src="@drawable/clock_icon"
        android:layout_below="@id/durationTextView"
        android:layout_alignParentStart="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        />
    <TextView
        android:id="@+id/timeTextViewTemp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Time : "
        android:textSize="16sp"
        android:layout_below="@id/durationTextView"
        android:layout_toEndOf="@id/locationImageView"
        android:layout_marginTop="20dp"
        android:layout_marginStart="10dp"/>

    <TextView
        android:id="@+id/timeTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:layout_below="@id/durationTextView"
        android:layout_toEndOf="@id/timeTextViewTemp"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="20dp"
        android:layout_marginStart="7dp"
        android:layout_marginEnd="10dp"
        android:textColor="#1A8E9E"
        />


    <ImageView
        android:id="@+id/locationImageView"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:src="@drawable/location_icon"
        android:layout_below="@id/timeTextView"
        android:layout_alignParentStart="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        />
    <TextView
        android:id="@+id/locationTextViewTemp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Location : "
        android:textSize="16sp"
        android:layout_below="@id/timeTextView"
        android:layout_toEndOf="@id/locationImageView"
        android:layout_marginTop="20dp"
        android:layout_marginStart="10dp"
        />

    <TextView
        android:id="@+id/locationTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:layout_below="@id/timeTextView"
        android:layout_toEndOf="@id/locationTextViewTemp"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="20dp"
        android:layout_marginStart="7dp"
        android:layout_marginEnd="10dp"
        android:textColor="#1A8E9E"
        />

    <ImageView
        android:id="@+id/collegeImageView"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:src="@drawable/college_icon"
        android:layout_below="@id/locationTextView"
        android:layout_alignParentStart="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"/>

    <TextView
        android:id="@+id/collegeTextViewTemp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Teacher Field : "
        android:textSize="16sp"
        android:layout_below="@id/locationTextView"
        android:layout_toEndOf="@id/collegeImageView"
        android:layout_marginTop="20dp"
        android:layout_marginStart="10dp"
        />

    <TextView
        android:id="@+id/collegeTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:layout_below="@id/locationTextView"
        android:layout_toEndOf="@id/collegeTextViewTemp"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="20dp"
        android:layout_marginStart="7dp"
        android:layout_marginEnd="10dp"
        android:textColor="#1A8E9E"
        />

    <ImageView
        android:id="@+id/teacherGenderImageView"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:src="@drawable/gender_icon"
        android:layout_below="@id/collegeTextView"
        android:layout_alignParentStart="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"/>

    <TextView
        android:id="@+id/teacherGenderTextViewTemp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Teacher Gender : "
        android:textSize="16sp"
        android:layout_below="@id/collegeTextView"
        android:layout_toEndOf="@id/teacherGenderImageView"
        android:layout_marginTop="20dp"
        android:layout_marginStart="10dp"
        />

    <TextView
        android:id="@+id/teacherGenderTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:layout_below="@id/collegeTextView"
        android:layout_toEndOf="@id/teacherGenderTextViewTemp"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="20dp"
        android:layout_marginStart="7dp"
        android:layout_marginEnd="10dp"
        android:textColor="#1A8E9E"
        />

    <ImageView
        android:id="@+id/teacherAgeImageView"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:src="@drawable/teacher_age_icon"
        android:layout_below="@id/teacherGenderTextView"
        android:layout_alignParentStart="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"/>

    <TextView
        android:id="@+id/teacherAgeTextViewTemp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Teacher Age : "
        android:textSize="16sp"
        android:layout_below="@id/teacherGenderTextView"
        android:layout_toEndOf="@id/teacherAgeImageView"
        android:layout_marginTop="20dp"
        android:layout_marginStart="10dp"
        />

    <TextView
        android:id="@+id/teacherAgeTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:layout_below="@id/teacherGenderTextView"
        android:layout_toEndOf="@id/teacherAgeTextViewTemp"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="20dp"
        android:layout_marginStart="7dp"
        android:layout_marginEnd="10dp"
        android:textColor="#1A8E9E"
        />

    <ImageView
        android:id="@+id/educationLevelImageView"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_below="@id/teacherAgeTextView"
        android:layout_toEndOf="@id/profileImageView"
        android:layout_alignParentStart="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/education_level_icon"/>
    <TextView
        android:id="@+id/educationLevelViewTemp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:text="Education Level :"
        android:layout_toEndOf="@id/educationLevelImageView"
        android:layout_marginStart="8dp"
        android:layout_below="@id/teacherAgeTextView"
        android:layout_marginTop="20dp"/>

    <TextView
        android:id="@+id/educationLevelTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:layout_toEndOf="@id/educationLevelViewTemp"
        android:layout_below="@id/teacherAgeTextView"
        android:layout_marginTop="20dp"
        android:layout_marginStart="8dp"
        android:textColor="#1A8E9E"
        android:layout_marginEnd="20dp"
        />

    <ImageView
        android:id="@+id/gradesImageView"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_below="@id/educationLevelTextView"
        android:layout_toEndOf="@id/profileImageView"
        android:layout_alignParentStart="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/education_level_icon"/>
    <TextView
        android:id="@+id/gradesViewTemp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:text="Grades :"
        android:layout_toEndOf="@id/gradesImageView"
        android:layout_marginStart="8dp"
        android:layout_below="@id/educationLevelTextView"
        android:layout_marginTop="20dp"/>

    <TextView
        android:id="@+id/gradesTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:layout_toEndOf="@id/gradesViewTemp"
        android:layout_below="@id/educationLevelTextView"
        android:layout_marginTop="20dp"
        android:layout_marginStart="8dp"
        android:textColor="#1A8E9E"
        android:layout_marginEnd="20dp"
        />



    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/sendMessageToTeacherBtn"
        android:layout_width="130dp"
        android:layout_height="60dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="10dp"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="10dp"
        android:text="Send Message"
        android:textSize="16sp"
        android:textAllCaps="false"
        android:layout_below="@id/gradesTextView"
        android:background="@drawable/rounded_corners"
        android:backgroundTint="@color/usedColor" />

    <androidx.appcompat.widget.AppCompatButton
        android:layout_width="140dp"
        android:layout_height="60dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="30dp"
        android:layout_marginBottom="10dp"
        android:layout_toStartOf="@+id/sendMessageToTeacherBtn"
        android:background="@drawable/rounded_corners"
        android:backgroundTint="@color/usedColor"
        android:layout_below="@id/gradesTextView"
        android:text="Send Request"
        android:textAllCaps="false"
        android:textSize="16sp" />




</RelativeLayout>